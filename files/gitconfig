[color]
    ui = true
    status = auto

[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true

[alias]
    unstage = reset -q HEAD --
    br = branch
    co = chechout
